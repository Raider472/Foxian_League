<?xml version="1.0" encoding="utf-8"?>
<!--Add the foxian language-->
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/FactionLanguages.LanguageFamilyDef[defName="Human"]/factionDialects</xpath>
        <value>
            <li>
                <dialectDef>FL_FoxianDialect</dialectDef>
                <factions>
                    <li>Foxian_League</li>
                    <li>Foxian_BloodRain</li>
                    <li>FoxianPlayerColony</li>
                </factions>
            </li>
        </value>
    </Operation>
</Patch>