<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="FoxianAmulet">
        <defName>FL_PsychicAmulet</defName>
        <label>psychic amulet</label>
        <description>A jade amulet.</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/SlaveCollar/SlaveCollar</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>0.8</drawSize>
        </graphicData>
        <costList>
            <Jade>20</Jade>
        </costList>
        <equippedStatOffsets>
            <PsychicSensitivityOffset>0.15</PsychicSensitivityOffset>
            <PsychicEntropyRecoveryRate>0.05</PsychicEntropyRecoveryRate>
            <MeditationFocusGain>0.1</MeditationFocusGain>
        </equippedStatOffsets>
    </ThingDef>

    <ThingDef ParentName="FoxianAmulet">
