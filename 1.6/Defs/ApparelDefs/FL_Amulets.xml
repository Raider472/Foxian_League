            <texPath>Things/Pawn/Humanlike/Apparel/SlaveCollar/SlaveCollar</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>0.8</drawSize>
        </graphicData>
        <costList>
            <Gold>20</Gold>
        </costList>
        <equippedStatOffsets>
            <NegotiationAbility>0.1</NegotiationAbility>
            <SocialImpact>0.1</SocialImpact>
        </equippedStatOffsets>
        <apparel>
            <tags>
                <li>FoxianLeagueAmulet</li>
            </tags>
        </apparel>
    </ThingDef>

