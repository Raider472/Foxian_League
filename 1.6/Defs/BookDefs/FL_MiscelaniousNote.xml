<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="BookBase">
        <defName>FL_MiscellaneousNote</defName>
        <label>a note</label>
        <description>a random note with things scribbled in it.</description>
        <graphicData>
            <texPath>Things/Item/Book/Schematic/Schematic</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>0.8</drawSize>
        </graphicData>
        <uiIconPath>Things/Item/Book/Schematic/Schematic</uiIconPath>
        <statBases>
            <MarketValue>100</MarketValue>
        </statBases>
        <comps>
            <li Class="CompProperties_Book">
                <nameMaker>FL_Namer_MiscellaneousNote</nameMaker>
                <descriptionMaker>FL_Description_MiscellaneousNote</descriptionMaker>
                <ageYearsRange>100~500</ageYearsRange>
                <openGraphic>
                    <texPath>Things/Item/Book/Schematic/Schematic_Open</texPath>
                    <graphicClass>Graphic_Multi</graphicClass>
                    <drawSize>0.7</drawSize>
                </openGraphic>
                <verticalGraphic>
                    <texPath>Things/Item/Book/Schematic/Schematic_Vertical</texPath>
                    <graphicClass>Graphic_Multi</graphicClass>
                    <addTopAltitudeBias>true</addTopAltitudeBias>
                </verticalGraphic>
                <doers>
                    <li Class="BookOutcomeProperties_JoyFactorModifier" />
                </doers>
            </li>
        </comps>
    </ThingDef>
</Defs>