<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef Name="FL_GeneEars" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<endogeneCategory>Ears</endogeneCategory>
		<displayCategory>FL_foxian_cosmetic</displayCategory>
		<displayOrderInCategory>70</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<li>Ears</li>
		</exclusionTags>
	</GeneDef>

	<GeneDef Name="FL_GeneTail" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<randomChosen>true</randomChosen>
		<displayCategory>FL_foxian_cosmetic</displayCategory>
		<displayOrderInCategory>71</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<exclusionTags>
			<li>Tail</li>
		</exclusionTags>
	</GeneDef>

	<GeneDef ParentName="FL_GeneEars">
		<defName>FL_FoxEar</defName>
		<label>Foxian ears</label>
		<description>Carriers of this gene have fox-like ears.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_Fox_Ears_Icon</iconPath>
		<renderNodeProperties>
			<li>
				<nodeClass>BetterGeneGraphicsFramework.PawnRenderNode_GeneHeadAttachment</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<parentTagDef>Head</parentTagDef>
				<texPaths>
					<li>Things/Pawns/Ears/FoxianEars</li>
					<li>Things/Pawns/Ears/FoxianLongEars</li>
				</texPaths>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useRottenColor>true</useRottenColor>
				<useSkinShader>false</useSkinShader>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<drawData>
					<defaultData>
						<layer>71</layer>
					</defaultData>
				</drawData>
			</li>
		</renderNodeProperties>
	</GeneDef>

	<GeneDef ParentName="FL_GeneTail">
		<defName>FL_FoxTail</defName>
		<label>Foxian tail</label>
		<description>Carriers of this gene have a fox-like tail.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_Fox_Tail_Icon</iconPath>

		<renderNodeProperties>
			<li>
				<nodeClass>BetterGeneGraphicsFramework.PawnRenderNode_GeneAttachment</nodeClass>
				<workerClass>BetterGeneGraphicsFramework.PawnRenderNodeWorker_GeneBodyAttachment</workerClass>
				<parentTagDef>Body</parentTagDef>
				<texPaths>
					<li>Things/Pawns/Tails/OneTail/FoxianOneTail</li>
					<li>Things/Pawns/Tails/OneTail/FoxianArchTail</li>
					<li>Things/Pawns/Tails/OneTail/FoxianFluffyTail</li>
				</texPaths>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useRottenColor>true</useRottenColor>
				<useSkinShader>false</useSkinShader>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<visibleFacing>
					<li>East</li>
					<li>West</li>
					<li>South</li>
					<li>North</li>
				</visibleFacing>
				<drawData>
					<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
					<defaultData>
						<layer>-2</layer>
					</defaultData>
					<dataSouth>
						<offset>(0, 0, -0.1)</offset>
					</dataSouth>
					<dataNorth>
						<layer>90</layer>
						<offset>(0, 0, -0.2)</offset>
					</dataNorth>
					<dataEast>
						<offset>(-0.25, 0, -0.15)</offset>
					</dataEast>
					<dataWest>
						<offset>(0.25, 0, -0.15)</offset>
					</dataWest>
				</drawData>
			</li>
		</renderNodeProperties>
		<modExtensions>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.WithBodyTypeOffset">
				<disableBodyTypeScaleForAdult>true</disableBodyTypeScaleForAdult>
				<bodyTypeOffsetNorth>
					<thin>(0.01,0,0)</thin>
					<hulk>(0,0,-0.13)</hulk>
					<child>(0.01,0,0.03)</child>
				</bodyTypeOffsetNorth>
				<bodyTypeOffsetEast>
					<male>(0.06,0,0.02)</male>
					<thin>(-0.02,0,0.04)</thin>
					<hulk>(-0.17,0,-0.11)</hulk>
					<fat>(-0.15,0,0.01)</fat>
					<child>(0.02,0,0.04)</child>
				</bodyTypeOffsetEast>
				<bodyTypeOffsetSouth>
					<male>(0,0,0.06)</male>
					<thin>(-0.06,0,0.04)</thin>
					<hulk>(0.02,0,-0.05)</hulk>
					<fat>(0.05,0,0.03)</fat>
					<child>(-0.03,0,0.08)</child>
				</bodyTypeOffsetSouth>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="FL_GeneTail">
		<defName>FL_NineFoxTail</defName>
		<label>Nine-tailed foxian tail</label>
		<description>Carriers of this gene have multiple fox-like tails. More tails grow the older they get.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_NineTailFox_Tail_Icon</iconPath>

		<renderNodeProperties>
			<li>
				<nodeClass>BetterGeneGraphicsFramework.PawnRenderNode_GeneAttachment</nodeClass>
				<workerClass>BetterGeneGraphicsFramework.PawnRenderNodeWorker_GeneBodyAttachment</workerClass>
				<parentTagDef>Body</parentTagDef>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useRottenColor>true</useRottenColor>
				<useSkinShader>false</useSkinShader>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<visibleFacing>
					<li>East</li>
					<li>West</li>
					<li>South</li>
					<li>North</li>
				</visibleFacing>
				<drawData>
					<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
					<defaultData>
						<layer>-1</layer>
					</defaultData>
					<dataSouth>
						<layer>-2</layer>
						<offset>(0, 0, -0.1)</offset>
					</dataSouth>
					<dataNorth>
						<layer>90</layer>
						<offset>(0, 0, -0.18)</offset>
					</dataNorth>
					<dataEast>
						<offset>(-0.5, 0, -0.1)</offset>
					</dataEast>
					<dataWest>
						<offset>(0.5, 0, -0.1)</offset>
					</dataWest>
				</drawData>
			</li>
		</renderNodeProperties>
		<modExtensions>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.GraphicsWithAge">
				<ages>
					<li>0</li>
					<li>15</li>
					<li>18</li>
					<li>21</li>
					<li>28</li>
				</ages>
				<graphicPaths>
					<li>Things/Pawns/Tails/MultipleTails/FoxianOneTail</li>
					<li>Things/Pawns/Tails/MultipleTails/FoxianThreeTails</li>
					<li>Things/Pawns/Tails/MultipleTails/FoxianFiveTails</li>
					<li>Things/Pawns/Tails/MultipleTails/FoxianSevenTails</li>
					<li>Things/Pawns/Tails/MultipleTails/FoxianNineTails</li>
				</graphicPaths>
			</li>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.ShaderSupport">
				<colorTwoType>Custom</colorTwoType>
				<colorTwo>(245, 245, 245)</colorTwo>
			</li>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.WithBodyTypeOffset">
				<disableBodyTypeScaleForAdult>true</disableBodyTypeScaleForAdult>
				<bodyTypeOffsetNorth>
					<hulk>(0,0,-0.04)</hulk>
					<child>(0.02,0,0.03)</child>
					<femaleBB MayRequire="ssulunge.BBBodySupport">(0,0,0)</femaleBB>
				</bodyTypeOffsetNorth>
				<bodyTypeOffsetEast>
					<hulk>(0,0,-0.06)</hulk>
					<child>(0.01,0,0.07)</child>
					<femaleBB MayRequire="ssulunge.BBBodySupport">(0,0,0)</femaleBB>
				</bodyTypeOffsetEast>
				<bodyTypeOffsetSouth>
					<hulk>(0,0,-0.06)</hulk>
					<fat>(0,0,0)</fat>
					<child>(-0.04,0,0.07)</child>
					<femaleBB MayRequire="ssulunge.BBBodySupport">(0,0,0)</femaleBB>
				</bodyTypeOffsetSouth>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>