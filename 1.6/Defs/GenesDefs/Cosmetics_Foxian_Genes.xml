<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef Name="FL_GeneEars" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<endogeneCategory>Ears</endogeneCategory>
		<displayCategory>FL_foxian_cosmetic</displayCategory>
		<displayOrderInCategory>70</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<li>Ears</li>
		</exclusionTags>
	</GeneDef>

	<GeneDef Name="FL_GeneTail" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<randomChosen>true</randomChosen>
		<displayCategory>FL_foxian_cosmetic</displayCategory>
		<displayOrderInCategory>71</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<exclusionTags>
			<li>Tail</li>
		</exclusionTags>
	</GeneDef>

	<GeneDef ParentName="FL_GeneEars">
		<defName>FL_FoxEar</defName>
		<label>Fox ears</label>
		<description>Carriers of this gene have fox-like ears.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_Fox_Ears_Icon</iconPath>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Things/Pawns/Ears/FL_Foxian_Ears</texPath>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useSkinShader>false</useSkinShader>
				<parentTagDef>Head</parentTagDef>
				<useRottenColor>true</useRottenColor>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<drawData>
					<defaultData>
					<layer>70</layer>
					</defaultData>
				</drawData>
			</li>
		</renderNodeProperties>
	</GeneDef>

	<GeneDef ParentName="FL_GeneTail">
		<defName>FL_FoxTail</defName>
		<label>Fox tail</label>
		<description>Carriers of this gene have a fox-like tail.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_Fox_Tail_Icon</iconPath>
		<renderNodeProperties>
			<li>
				<workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
				<parentTagDef>Body</parentTagDef>
				<texPath>Things/Pawns/Tails/Female/FoxianOneTail_Female</texPath>
				<bodyTypeGraphicPaths>
					<Male>Things/Pawns/Tails/Male/FoxianNineTails_Male</Male>
					<Female>Things/Pawns/Tails/Female/FoxianNineTails_Female</Female>
					<Thin>Things/Pawns/Tails/Thin/FoxianNineTails_Thin</Thin>
					<Hulk>Things/Pawns/Tails/Hulk/FoxianFiveTails_Hulk</Hulk>
					<Fat>Things/Pawns/Tails/Fat/FoxianFiveTails_Fat</Fat>
				</bodyTypeGraphicPaths>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useRottenColor>true</useRottenColor>
				<useSkinShader>false</useSkinShader>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<visibleFacing>
					<li>East</li>
					<li>West</li>
					<li>South</li>
					<li>North</li>
				</visibleFacing>
				<drawData>
					<defaultData>
						<layer>1</layer>
					</defaultData>
					<dataSouth>
						<layer>-2</layer>
					</dataSouth>
					<dataNorth>
						<layer>90</layer>
					</dataNorth>
					<dataEast>
						<layer>70</layer>
					</dataEast>
				</drawData>
			</li>
		</renderNodeProperties>
	</GeneDef>

	<GeneDef ParentName="FL_GeneTail">
		<defName>FL_NineFoxTail</defName>
		<label>Nine Tailed Fox tail</label>
		<description>Carriers of this gene have multiple fox-like tails. More tails grow the older they get.</description>
		<iconPath>UI/Icons/Genes/FL_Gene_NineTailFox_Tail_Icon</iconPath>

		<renderNodeProperties>
			<li>
				<nodeClass>BetterGeneGraphicsFramework.PawnRenderNode_GeneAttachment</nodeClass>
				<workerClass>BetterGeneGraphicsFramework.PawnRenderNodeWorker_GeneBodyAttachment</workerClass>
				<parentTagDef>Body</parentTagDef>
				<shaderTypeDef>CutoutComplex</shaderTypeDef>
				<colorType>Hair</colorType>
				<useRottenColor>true</useRottenColor>
				<useSkinShader>false</useSkinShader>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<visibleFacing>
					<li>East</li>
					<li>West</li>
					<li>South</li>
					<li>North</li>
				</visibleFacing>
				<drawData>
					<defaultData>
						<layer>1</layer>
					</defaultData>
					<dataSouth>
						<layer>-2</layer>
					</dataSouth>
					<dataNorth>
						<layer>90</layer>
					</dataNorth>
					<dataEast>
						<layer>71</layer>
					</dataEast>
				</drawData>
			</li>
		</renderNodeProperties>
		<modExtensions>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.GraphicsWithAge">
				<ages>
					<li>0</li>
					<li>15</li>
					<li>20</li>
					<li>30</li>
					<li>35</li>
				</ages>
				<graphicPaths>
					<li>Things/Pawns/Tails/Female/FoxianOneTail_Female</li>
					<li>Things/Pawns/Tails/Female/FoxianThreeTails_Female</li>
					<li>Things/Pawns/Tails/Female/FoxianFiveTails_Female</li>
					<li>Things/Pawns/Tails/Female/FoxianSevenTails_Female</li>
					<li>Things/Pawns/Tails/Female/FoxianNineTails_Female</li>
				</graphicPaths>
				<bodyTypeGraphicPaths>
					<Female>Things/Pawns/Tails/Female/FoxianOneTail_Female</Female>
					<Female>Things/Pawns/Tails/Female/FoxianThreeTails_Female</Female>
					<Female>Things/Pawns/Tails/Female/FoxianFiveTails_Female</Female>
					<Female>Things/Pawns/Tails/Female/FoxianSevenTails_Female</Female>
					<Female>Things/Pawns/Tails/Female/FoxianNineTails_Female</Female>

					<Male>Things/Pawns/Tails/Male/FoxianOneTail_Male</Male>
					<Male>Things/Pawns/Tails/Male/FoxianThreeTails_Male</Male>
					<Male>Things/Pawns/Tails/Male/FoxianFiveTails_Male</Male>
					<Male>Things/Pawns/Tails/Male/FoxianSevenTails_Male</Male>
					<Male>Things/Pawns/Tails/Male/FoxianNineTails_Male</Male>

					<Thin>Things/Pawns/Tails/Thin/FoxianOneTail_Thin</Thin>
					<Thin>Things/Pawns/Tails/Thin/FoxianThreeTails_Thin</Thin>
					<Thin>Things/Pawns/Tails/Thin/FoxianFiveTails_Thin</Thin>
					<Thin>Things/Pawns/Tails/Thin/FoxianSevenTails_Thin</Thin>
					<Thin>Things/Pawns/Tails/Thin/FoxianNineTails_Thin</Thin>

					<Hulk>Things/Pawns/Tails/Hulk/FoxianOneTail_Hulk</Hulk>
					<Hulk>Things/Pawns/Tails/Hulk/FoxianThreeTails_Hulk</Hulk>
					<Hulk>Things/Pawns/Tails/Hulk/FoxianFiveTails_Hulk</Hulk>
					<Hulk>Things/Pawns/Tails/Hulk/FoxianSevenTails_Hulk</Hulk>
					<Hulk>Things/Pawns/Tails/Hulk/FoxianNineTails_Hulk</Hulk>

					<Fat>Things/Pawns/Tails/Fat/FoxianOneTail_Fat</Fat>
					<Fat>Things/Pawns/Tails/Fat/FoxianThreeTails_Fat</Fat>
					<Fat>Things/Pawns/Tails/Fat/FoxianFiveTails_Fat</Fat>
					<Fat>Things/Pawns/Tails/Fat/FoxianSevenTails_Fat</Fat>
					<Fat>Things/Pawns/Tails/Fat/FoxianNineTails_Fat</Fat>
				</bodyTypeGraphicPaths>
			</li>
			<li MayRequire="telardo.BetterGeneGraphicsFramework" Class="BetterGeneGraphicsFramework.ShaderSupport">
				<colorTwoType>Custom</colorTwoType>
				<colorTwo>(245, 245, 245)</colorTwo>
			</li>
		</modExtensions>
	</GeneDef>

</Defs>